    <img src="https://via.placeholder.com/300x150" alt="Card 1">
    <div class="card-body">
        <h5 class="card-title">Card 1</h5>
        <a href="@Url.Action("PhysicianIndex", "Admin")" class="btn btn-primary">Physician</a>
